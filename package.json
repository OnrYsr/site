{
  "name": "muse3dstudio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed:products": "ts-node prisma/seed-products.ts",
    "db:seed": "npm run db:seed:products",
    "make:admin": "node scripts/make-admin.js",
    "create:admin": "node scripts/create-admin.js",
    "check:user": "node scripts/check-user.js",
    "db:setup-production": "chmod +x scripts/setup-database.sh && ./scripts/setup-database.sh",
    "setup:server": "chmod +x scripts/setup-server.sh && sudo ./scripts/setup-server.sh",
    "setup:nginx": "chmod +x scripts/setup-nginx.sh && sudo ./scripts/setup-nginx.sh",
    "deploy:full": "npm install && npm run build && pm2 start ecosystem.config.js && pm2 save",
    "deploy:update": "git pull && npm install && npm run build && pm2 restart muse3dstudio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.10.1",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.20.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "next-auth": "^4.24.11",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.59.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
